            <TextView android:paddingLeft="10sp" android:text="Data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="14sp" android:textColor="#ffeeeeee" android:id="@+id/info_dialog_zipcode"></TextView>
