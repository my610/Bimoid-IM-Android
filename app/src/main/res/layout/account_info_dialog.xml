                <TextView android:text="Имя" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="18sp" android:textColor="#ffffffff" android:id="@+id/l1"></TextView>
                <LinearLayout android:layout_marginBottom="5sp" android:orientation="vertical" android:layout_marginTop="7sp" android:layout_marginLeft="10sp" android:layout_width="fill_parent" android:layout_height="1sp" android:layout_marginRight="10sp"></LinearLayout>
            </LinearLayout>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/info_dialog_firstname" android:text="First name" android:paddingLeft="10sp" android:textSize="14sp" android:textColor="#ffeeeeee"></TextView>
            <LinearLayout android:gravity="center_vertical" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="10sp">
                <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="18sp" android:textColor="#ffffffff" android:text="Фамилия" android:id="@+id/l2"></TextView>
                <LinearLayout android:layout_marginBottom="5sp" android:orientation="vertical" android:layout_marginTop="7sp" android:layout_marginLeft="10sp" android:layout_width="fill_parent" android:layout_height="1sp" android:layout_marginRight="10sp"></LinearLayout>
            </LinearLayout>
            <TextView android:paddingLeft="10sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="14sp" android:textColor="#ffeeeeee" android:text="Data" android:id="@+id/info_dialog_lastname"></TextView>
            <LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:gravity="center_vertical" android:layout_marginTop="10sp">
                <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#ffffffff" android:textSize="18sp" android:text="Страна" android:id="@+id/l3"></TextView>
                <LinearLayout android:layout_marginBottom="5sp" android:orientation="vertical" android:layout_marginLeft="10sp" android:layout_width="fill_parent" android:layout_height="1sp" android:layout_marginRight="10sp" android:layout_marginTop="7sp"></LinearLayout>
            </LinearLayout>
            <TextView android:paddingLeft="10sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="14sp" android:textColor="#ffeeeeee" android:id="@+id/info_dialog_country" android:text="Data"></TextView>
            <LinearLayout android:gravity="center_vertical" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="10sp">
