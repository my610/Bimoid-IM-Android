            </LinearLayout>
            <TextView android:paddingLeft="10sp" android:text="Data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="14sp" android:textColor="#ffeeeeee" android:id="@+id/info_dialog_address"></TextView>
            <LinearLayout android:gravity="center_vertical" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="10sp">
                <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="18sp" android:textColor="#ffffffff" android:text="Языки" android:id="@+id/l8"></TextView>
                <LinearLayout android:layout_marginBottom="5sp" android:orientation="vertical" android:layout_marginTop="7sp" android:layout_marginLeft="10sp" android:layout_width="fill_parent" android:layout_height="1sp" android:layout_marginRight="10sp"></LinearLayout>
            </LinearLayout>
            <TextView android:paddingLeft="10sp" android:text="Data" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="14sp" android:textColor="#ffeeeeee" android:id="@+id/info_dialog_languages"></TextView>
            <LinearLayout android:gravity="center_vertical" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="10sp">
