                <LinearLayout android:layout_marginBottom="5sp" android:orientation="vertical" android:layout_marginTop="7sp" android:layout_marginLeft="10sp" android:layout_width="fill_parent" android:layout_height="1sp" android:layout_marginRight="10sp"></LinearLayout>
