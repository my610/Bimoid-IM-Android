<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ru.ivansuper.Preferences.LanguagePicker android:key="ms_select_language" />
    <PreferenceScreen android:key="ms_accounts">
        <intent android:action="ru.ivansuper.bimoidim.PROFILES" />

    </PreferenceScreen>
    <PreferenceScreen android:key="ms_skins">
        <intent android:action="ru.ivansuper.bimoidim.SKINS" />
    </PreferenceScreen>
    <PreferenceScreen android:key="ms_contactlist">
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="ms_hide_empty_groups" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="ms_groups" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="ms_offline" />
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="ms_show_addit_desc" />

    </PreferenceScreen>
    <PreferenceScreen android:key="ms_notify">
        <CheckBoxPreference android:key="ms_vibro">

        </CheckBoxPreference>
        <PreferenceCategory android:key="ms_snd_category" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="ms_sounds" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="auth_a_s" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="auth_d_s" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="auth_r_s" />

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="auth_re_s" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="inc_msg_s"
            android:title="Входящее сообщение" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="sent_s" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="info_s"
            android:title="Информация" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="svc_s" />

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="in_s" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="out_s" />
        <PreferenceCategory android:key="ms_bar_category">

        </PreferenceCategory>
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="ms_personal_notify" />


    </PreferenceScreen>
    <PreferenceScreen android:key="ms_power">

        <CheckBoxPreference
            android:defaultValue="false"
            android:key="ms_wake_lock" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="ms_wifi_lock" />

    </PreferenceScreen>
    <PreferenceScreen android:key="ms_antispam">
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="antispam_enabled" />
        <EditTextPreference
            android:defaultValue="[EN] Antispam: What is the biggest country in the world?_[RU] Антиспам: Самая большая по площади страна в мире?"
            android:key="antispam_question" />
        <EditTextPreference
            android:defaultValue="Russia, Россия"
            android:key="antispam_answer" />
        <EditTextPreference
            android:defaultValue="[EN] Thank you. Now you can send messages directly to my contact list._[RU] Спасибо. Теперь вы можете писать прямо мне."
            android:key="antispam_accepted_msg" />

    </PreferenceScreen>
    <PreferenceScreen android:key="ms_history">
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="ms_store_history" />
    </PreferenceScreen>

</PreferenceScreen>
